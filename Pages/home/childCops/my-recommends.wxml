<!--Pages/home/childCops/my-recommends.wxml-->
<view class="recommend">
  <block wx:for="{{recommends}}" wx:key="{{index}}">
    <view class="recommend-item">
      <image src="{{item.image}}" mode="widthFix" bindload="handleImageLoad"/>
      <view>{{item.title}}</view>
    </view>
  </block>
</view>